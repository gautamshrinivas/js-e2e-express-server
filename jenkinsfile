pipeline {
    agent { label 'node'}
    stages {
        stage ('vcs') {
            steps {
            git url: 'https://github.com/gautamshrinivas/js-e2e-express-server.git',
            branch: 'REL'
            }
        }
        stage ('build') {
            steps {
                sh 'npm install'
                sh 'npm run build'
                
            }
        }

    }
}